<Application x:Class="CarService.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CarService"
             xmlns:svgc = "http://sharpvectors.codeplex.com/svgc/"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="MainDictionary.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Themes/LightTheme.xaml"></ResourceDictionary>
                <ResourceDictionary Source="DictionaryIcon.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/AuthorizRegDictionary.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/PagesFormsDictionary.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
